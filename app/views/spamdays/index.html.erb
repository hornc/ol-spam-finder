
<!-- <h1><%= @month + ' ' +  @year %></h1> -->

<%= month_calendar start_date: @start_date, 
     td: month_calendar_td_options,
     previous_link: link_to(raw("&laquo;"), @previous),
     next_link: link_to(raw("&raquo;"), @next) do |date| %>
  <% #if date.month == Date.parse(@start_date).month %>
     <%= link_to date.day, "#{@start_date[0..-4]}/#{date.day}" %>
  
  <% #end %>
<% end %>
